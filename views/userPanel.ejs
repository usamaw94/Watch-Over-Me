<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<link rel="apple-touch-icon" sizes="76x76" href="userPanelAssets/img/apple-icon.png">
	<link rel="icon" type="image/png" sizes="96x96" href="userPanelAssets/img/favicon.png">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>Paper Dashboard by Creative Tim</title>

	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


    <!-- Bootstrap core CSS     -->
    <link href="userPanelAssets/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Animation library for notifications   -->
    <link href="userPanelAssets/css/animate.min.css" rel="stylesheet"/>

    <!--  Paper Dashboard core CSS    -->
    <link href="userPanelAssets/css/paper-dashboard.css" rel="stylesheet"/>


    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="userPanelAssets/css/demo.css" rel="stylesheet" />

    <link href="userPanelAssets/css/custom.css" rel="stylesheet" />

    <!--  Fonts and icons     -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
    <link href="userPanelAssets/css/themify-icons.css" rel="stylesheet">

</head>
<body>

<div class="wrapper">
    <div class="sidebar" data-background-color="white" data-active-color="danger">

    <!--
		Tip 1: you can change the color of the sidebar's background using: data-background-color="white | black"
		Tip 2: you can change the color of the active button using the data-active-color="primary | info | success | warning | danger"
	-->

        <div class="sidebar-wrapper">
            <div class="logo">
                <a href="http://www.creative-tim.com" class="simple-text">
                    Watch Over ME
                </a>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="/userPanel">
                        <i class="ti-panel"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <li>
                    <a href="/wearerServiceDetails">
                        <i class="ti-settings"></i>
                        <p>Service as Wearer</p>
                    </a>
                </li>
                <li>
                    <a href="servicesList.html">
                        <i class="ti-settings"></i>
                        <p>Services as Watcher</p>
                    </a>
                </li>
                <li>
                    <a href="/customerServiceList">
                        <i class="ti-settings"></i>
                        <p>Services as Customer</p>
                    </a>
                </li>
                <li>
                    <a href="/userProfile">
                        <i class="ti-user"></i>
                        <p>User Profile</p>
                    </a>
                </li>
                <li>
                    <a href="/logout">
                        <i class="ti-close"></i>
                        <p>Logout</p>
                    </a>
                </li>
            </ul>
        </div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar bar1"></span>
                        <span class="icon-bar bar2"></span>
                        <span class="icon-bar bar3"></span>
                    </button>
                    <a class="navbar-brand" href="#">Dashboard</a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
						<li>
                            <a href="#">
								<i class="ti-user"></i>
								<p><%= session.userName %></p>
                            </a>
                        </li>
                    </ul>

                </div>
            </div>
        </nav>


        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-4 col-sm-12">
                        <div class="card">
                            <div class="content">
                                <div class="row">
                                    <div class="col-xs-5">
                                        <div class="icon-big icon-warning text-center">
                                            <i class="ti-settings"></i>
                                        </div>
                                    </div>
                                    <div class="col-xs-7">
                                        <div class="numbers">
                                            <p>Service as</p>
                                            Wearer
                                        </div>
                                    </div>
                                </div>
                                <div class="footer">
                                    <hr />
                                    <div class="stats">
                                        <%if (locals.wearerServiceData) { %>

                                            <a href="/wearerServiceDetails"><i class="ti-eye"></i> View</a>

                                        <% } else { %>
                                            
                                            <p><i class="ti-close"></i> No data to show</p>

                                        <% } %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6">
                        <div class="card">
                            <div class="content">
                                <div class="row">
                                    <div class="col-xs-5">
                                        <div class="icon-big icon-success text-center">
                                            <i class="ti-settings"></i>
                                        </div>
                                    </div>
                                    <div class="col-xs-7">
                                        <div class="numbers">
                                            <p>Services as</p>
                                            Watcher
                                        </div>
                                    </div>
                                </div>
                                <div class="footer">
                                    <hr />
                                    <div class="stats">
                                        <div class="stats">
                                            <%if (locals.watcherServiceData) { %>

                                                <a href="/customerServiceList"><i class="ti-eye"></i> View All</a>
    
                                            <% } else { %>
                                                
                                                <p><i class="ti-close"></i> No data to show</p>
    
                                            <% } %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-sm-6">
                        <div class="card">
                            <div class="content">
                                <div class="row">
                                    <div class="col-xs-5">
                                        <div class="icon-big icon-danger text-center">
                                            <i class="ti-settings"></i>
                                        </div>
                                    </div>
                                    <div class="col-xs-7">
                                        <div class="numbers">
                                            <p>Services as</p>
                                            Customer
                                        </div>
                                    </div>
                                </div>
                                <div class="footer">
                                    <hr />
                                    <div class="stats">
                                            <%if (locals.customerServiceData) { %>

                                                <a href="/customerServiceList"><i class="ti-eye"></i> View All</a>
    
                                            <% } else { %>
                                                
                                                <p><i class="ti-close"></i> No data to show</p>
    
                                            <% } %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">

                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <p class="category">Service as</p>
                                <h4 class="title">Wearer</h4>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table table-striped">
                                    <%if (locals.wearerServiceData) { %>
                                    <thead>
                                        <th>ID</th>
                                        <th>WOM Number</th>
                                        <th>Customer Name</th>
                                        <th>Number of Watchers</th>
                                        <th>Status</th>
                                        <th></th>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td><%= wearerServiceData[0].serviceId %></td>
                                        <td><%= wearerServiceData[0].womNumber %></td>
                                        <td><span data-id="<%= wearerServiceData[0].customers[0].person_id %>" class="showCustomerDetails show-full-details"><%= wearerServiceData[0].customers[0].person_first_name %> <%= wearerServiceData[0].customers[0].person_last_name %></span></td>
                                        <td><span data-id="<%= wearerServiceData[0].serviceId %>" data-num-watcher="<%= wearerServiceData[0].numberOfWatchers %>" class="showWatcherDetails show-full-details"><%= wearerServiceData[0].numberOfWatchers %></span></td>
                                        <td><%= wearerServiceData[0].serviceStatus %></td>
                                        <td>
                                            <a class="btn btn-info" href="/wearerServiceDetails">View details</a>
                                        </td>
                                    </tr>
                                    </tbody>
                                    <% } else { %>
                                        <centre>
                                            <h6 class="text-center info" style="margin-left:20px; color: #F3BB45">User is not the wearer for any service</h6>
                                        </centre>
                                    <% } %>
                                    
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="card">
                            <div class="header">
                                <div style="float: right" class="text-right">
                                    <%if (locals.watcherServiceData) { %>
                                    <a href="servicesList.html" class="btn btn-sm btn-primary btn-icon">View all</a>
                                    <% } %>
                                </div>
                                <p class="category">Services as</p>
                                <h4 class="title">Watcher</h4>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table table-striped">
                                    <%if (locals.watcherServiceData) { %>
                                    <thead>
                                    <th>ID</th>
                                    <th>WOM Number</th>
                                    <th>Status</th>
                                    <th></th>
                                    </thead>
                                    <tbody>
                                    <% for(var wtsd of watcherServiceData) {%>
                                        <tr>
                                            <td><%= wtsd.serviceId %></td>
                                            <td><%= wtsd.womNumber %></td>
                                            <td><%= wtsd.serviceStatus %></td>
                                            <td>
                                                <a class="btn btn-info" href="serviceLogs.html">Details</a>
                                            </td>
                                        </tr>
                                    <% } %>
                                    </tbody>
                                    <% } else { %>
                                        <centre>
                                            <h6 class="text-center info" style="margin-left:20px; color: #F3BB45">User is not the watcher for any service</h6>
                                        </centre>
                                    <% } %>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="card">
                            <div class="header">
                                <div style="float: right" class="text-right">
                                    <%if (locals.customerServiceData) { %>
                                    <a href="/customerServiceList" class="btn btn-sm btn-primary btn-icon">View all</a>
                                    <% } %>
                                </div>
                                <p class="category">Services as</p>
                                <h4 class="title">Customer</h4>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table table-striped">
                                    <%if (locals.customerServiceData) { %>
                                    <thead>
                                    <th>ID</th>
                                    <th>WOM Number</th>
                                    <th>Status</th>
                                    <th></th>
                                    </thead>
                                    <tbody>
                                    <% for(var csd of customerServiceData) {%>
                                    <tr>
                                        <td><%= csd.service_id %></td>
                                        <td><%= csd.wom_num %></td>
                                        <td><%= csd.status %></td>
                                        <td>
                                            <a class="btn btn-info" href="/customerServiceDetails?serviceID=<%= csd.service_id %>">Details</a>
                                        </td>
                                    </tr>
                                    <% } %>
                                    </tbody>
                                    <% } else { %>
                                        <centre>
                                            <h6 class="text-center info" style="margin-left:20px; color: #F3BB45">User is not the customer for any service</h6>
                                        </centre>
                                    <% } %>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <footer class="footer">
            <div class="container-fluid">
                <nav class="pull-left">
                    <ul>

                        <li>
                            <a href="http://www.creative-tim.com">
                                Creative Tim
                            </a>
                        </li>
                        <li>
                            <a href="http://blog.creative-tim.com">
                               Blog
                            </a>
                        </li>
                        <li>
                            <a href="http://www.creative-tim.com/license">
                                Licenses
                            </a>
                        </li>
                    </ul>
                </nav>
                <div class="copyright pull-right">
                    &copy; <script>document.write(new Date().getFullYear())</script>, made with <i class="fa fa-heart heart"></i> by <a href="http://www.creative-tim.com">Creative Tim</a>
                </div>
            </div>
        </footer>

    </div>
</div>

<div class="modal fade" id="customerDetailsModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <span class="modal-custom-title modal-title">Customer Details</span>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <table class="table table-striped table-responsive">
                    <thead>
                    <tr>
                        <th>Wearer_ID:</th>
                        <td id="modalCustomerId"></td>
                    </tr>
                    <tr>
                        <th>Name:</th>
                        <td id="modalCustomerName"></td>
                    </tr>
                    <tr>
                        <th>Phone Number:</th>
                        <td id="modalCustomerPhone"></td>
                    </tr>
                    <tr>
                        <th>Email:</th>
                        <td id="modalCustomerEmail"></td>
                    </tr>
                    </thead>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="watcherDetailsModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <span class="modal-custom-title modal-title">Watcher Details</span>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <h6>Total watchers : <span id="modalTotalWatchers"></span></h6>
                <table class="table table-striped table-responsive">
                    <thead>
                    <tr>
                        <th>Watcher_ID:</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Phone</th>
                    </tr>
                    </thead>
                    <tbody id="watcherDataRows">

                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


    <!--   Core JS Files   -->
    <script src="userPanelAssets/js/jquery.min.js" type="text/javascript"></script>
	<script src="userPanelAssets/js/bootstrap.min.js" type="text/javascript"></script>

	<!--  Checkbox, Radio & Switch Plugins -->
	<script src="userPanelAssets/js/bootstrap-checkbox-radio.js"></script>

	<!--  Charts Plugin -->
	<script src="userPanelAssets/js/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="userPanelAssets/js/bootstrap-notify.js"></script>

    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

    <!-- Paper Dashboard Core javascript and methods for Demo purpose -->
	<script src="userPanelAssets/js/paper-dashboard.js"></script>

	<!-- Paper Dashboard DEMO methods, don't include it in your project! -->
    <script src="userPanelAssets/js/demo.js"></script>
    
    <script src="userPanelAssets/js/custom.js"></script>

</body>

</html>
