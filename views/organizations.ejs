      <!-- Main content -->
      <section class="content">
        <div class="container-fluid">
          <!-- Small boxes (Stat box) -->
  
          <div class="row">
            <div class="col-lg-12 col-12">
              <div class="card card-info">
                <div class="card-header">
                  <h4 class="card-title">
                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                      <i class="fa fa-plus"></i> &nbsp;
                      Add new Organization
                    </a>
                  </h4>
                </div>
                <div id="collapseOne" class="panel-collapse collapse in">
                  <div class="card-body">
                    <form id="addOrganizationForm">
                      <div class="form-group">
                        <label>Name</label>
                        <input type="text" name="orgName" class="form-control" placeholder="Enter ..." required>
                      </div>

                      <div class="form-group">
                        <label>Phone number</label>
                        <input type="number" name="orgPhone" step="any" class="form-control" placeholder="Enter ..." required>
                      </div>
  
                      <div class="form-group">
                        <label>Email</label>
                        <input type="email" name="orgEmail" class="form-control" placeholder="Enter ..." required>
                      </div>

                      <div class="form-group">
                        <label>Address</label>
                        <textarea required name="orgAddress" class="form-control" placeholder="Enter ..." required></textarea>
                      </div>

                      <div class="form-group">
                        <label>Reference number</label>
                        <input type="text" name="orgReference" class="form-control" placeholder="Enter ..." required>
                      </div>
  
                      <div class="form-group">
                          <label>Is Pharmacy?</label><br>
                          <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="isPharmacy" id="inlineCheckbox1" value="yes">
                            <label class="form-check-label" for="inlineCheckbox1">Yes</label>
                          </div>
                          <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="isPharmacy" id="inlineCheckbox2" value="no" checked>
                            <label class="form-check-label" for="inlineCheckbox2">No</label>
                          </div>
                        </div>

                      <div class="form-group">
                        <label>Primary contact name</label>
                        <input type="text" class="form-control" name="orgPrimaryContact" placeholder="Enter ..." required>
                      </div>
  
                      <div class="form-group">
                        <label>Primary contact name</label>
                        <input type="text" class="form-control" name="orgSecondaryContact" placeholder="Enter ..." required>
                      </div>
  
                      <div class="form-group">
                        <input type="submit" class="btn btn-block btn-outline-info" value="Add Pharmacy">
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-lg-12 col-12">
              <div class="card card-outline card-info">
                <div class="card-header">
                  <h3 class="card-title">Organization List</h3>
                </div>
                <!-- /.card-header -->
                    <!-- text input -->
              <div id="reload">
                <div class="card-body table-responsive p-0">
                  <table class="table table-hover">
                      <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Phone</th>
                        <th>Email</th>
                        <th>Contact Person</th>
                        <th>Pharmacy</th>
                        <th></th>
                      </tr>
                      <% for(var d of data) {%>
                      <tr>
                        <td><%= d.organization_id %></td>
                        <td><%= d.organization_name %></td>
                        <td><%= d.organization_phone %></td>
                        <td><%= d.organization_email %></td>
                        <td><%= d.org_primary_contact_name %></td>
                        <td><%= d.is_pharmacy %></td>
                        <td><a class="btn btn-info" href="">View Details</a></td>
                      </tr>
                      <% } %>
                    </table>
                </div>
              </div>
                <!-- /.card-body -->
              </div>
            </div>
          </div>
  
        </div>
